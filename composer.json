{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "algolia/algoliasearch-client-php": "^1.18",
        "barryvdh/laravel-ide-helper": "^2.3",
        "doctrine/dbal": "^2.5",
        "graham-campbell/markdown": "^7.1",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.4.*",
        "laravel/scout": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelista/sherlock": "^0.5.3",
        "madeitbelgium/laravel-email-domain-validation": "1.*",
        "spatie/laravel-medialibrary": "^5.0.0",
        "tymon/jwt-auth": "0.5.*",
        "yajra/laravel-datatables-html": "^2.0",
        "yajra/laravel-datatables-oracle": "^7.5",
        "zizaco/entrust": "1.9.0-beta1",
        "league/flysystem-aws-s3-v3": "~1.0",
        "ext-gd": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ],
        "post-deploy-cmd": [
            "chmod -R 755 bootstrap\/cache"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
